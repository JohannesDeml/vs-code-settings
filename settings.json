{
    "telemetry.enableTelemetry": false,
    "telemetry.enableCrashReporter": false,
    "typescript.updateImportsOnFileMove.enabled": "always",
    "editor.mouseWheelZoom": true,
    "editor.renderWhitespace": "boundary",
    "files.trimTrailingWhitespace": true,
    "explorer.confirmDragAndDrop": false,
    "explorer.confirmDelete": false,
    "typescript.preferences.importModuleSpecifier": "relative",
    "editor.suggestSelection": "first",
    "files.exclude": {
        "**/.classpath": true,
        "**/.project": true,
        "**/.settings": true,
        "**/.factorypath": true
    },
    "workbench.startupEditor": "none",
    "security.workspace.trust.untrustedFiles": "open",
    "diffEditor.maxComputationTime": 0,
    "files.associations": {
        "*.asset": "yaml",
        "*.meta": "yaml",
        "*.prefab": "yaml",
        "*.unity": "yaml",
        "*.jsont": "json"
    },
    "yaml.customTags": [
        "tag:unity3d.com,2011:114 mapping"
    ],
    "redhat.telemetry.enabled": false,
    "diffEditor.renderSideBySide": false,
    "cSpell.userWords": [
        "Behaviour"
    ]
    "fileHeaderComment.parameter":{
        "*":{
            "commentbegin": "// --------------------------------------------------------------------------------------------------------------------",
            "commentprefix": "// ",
            "commentend": "// --------------------------------------------------------------------------------------------------------------------",
            "company": "Johannes Deml",
            "name": "Johannes Deml",
            "email": "public@deml.io"
        }
    },
    "fileHeaderComment.template":{
        "*":[
            "${commentbegin}",
            "${commentprefix}<copyright file=\"${filename}\">",
            "${commentprefix}  Copyright (c) ${year} ${company}. All rights reserved.",
            "${commentprefix}</copyright>",
            "${commentprefix}<author>",
            "${commentprefix}  ${name}",
            "${commentprefix}  ${email}",
            "${commentprefix}</author>",
            "${commentend}"
        ]
    }
}
