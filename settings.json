{
    "telemetry.enableTelemetry": false,
    "telemetry.enableCrashReporter": false,
    "typescript.updateImportsOnFileMove.enabled": "always",
    "editor.mouseWheelZoom": true,
    "editor.renderWhitespace": "boundary",
    "files.trimTrailingWhitespace": true,
    "explorer.confirmDragAndDrop": false,
    "explorer.confirmDelete": false,
    "diffEditor.codeLens": true,
    "git.mergeEditor": false,
    "typescript.preferences.importModuleSpecifier": "relative",
    "editor.suggestSelection": "first",
    "files.exclude": {
        "**/.classpath": true,
        "**/.project": true,
        "**/.settings": true,
        "**/.factorypath": true
    },
    "workbench.startupEditor": "none",
    "security.workspace.trust.untrustedFiles": "open",
    "diffEditor.maxComputationTime": 0,
    "files.associations": {
        "*.asset": "yaml",
        "*.meta": "yaml",
        "*.prefab": "yaml",
        "*.unity": "yaml",
        "*.playable": "yaml",
        "*.controller": "yaml",
        "*.jsont": "json",
        "*.uxml": "xml",
        "*.uss": "css"
    },
    "yaml.customTags": [
        "tag:unity3d.com,2011:1 mapping",
        "tag:unity3d.com,2011:2 mapping",
        "tag:unity3d.com,2011:3 mapping",
        "tag:unity3d.com,2011:4 mapping",
        "tag:unity3d.com,2011:20 mapping",
        "tag:unity3d.com,2011:29 mapping",
        "tag:unity3d.com,2011:95 mapping",
        "tag:unity3d.com,2011:104 mapping",
        "tag:unity3d.com,2011:114 mapping",
        "tag:unity3d.com,2011:157 mapping",
        "tag:unity3d.com,2011:196 mapping",
        "tag:unity3d.com,2011:222 mapping",
        "tag:unity3d.com,2011:223 mapping",
        "tag:unity3d.com,2011:224 mapping",
        "tag:unity3d.com,2011:225 mapping",
        "tag:unity3d.com,2011:1001 mapping",
    ],
    "[yaml]": {
        "files.trimTrailingWhitespace": false
    },
    "[po]": {
        "files.trimTrailingWhitespace": false
    },
    "redhat.telemetry.enabled": false,
    "cSpell.userWords": [
        "Behaviour",
        "Decompilation",
        "Deml",
        "InnoGames"
    ],
    "fileHeaderComment.parameter":{
        "*":{
            "commentbegin": "// --------------------------------------------------------------------------------------------------------------------",
            "commentprefix": "// ",
            "commentend": "// --------------------------------------------------------------------------------------------------------------------",
            "company": "Johannes Deml",
            "name": "Johannes Deml",
            "email": "public@deml.io"
        }
    },
    "fileHeaderComment.template":{
        "*":[
            "${commentbegin}",
            "${commentprefix}<copyright file=\"${filename}\">",
            "${commentprefix}  Copyright (c) ${year} ${company}. All rights reserved.",
            "${commentprefix}</copyright>",
            "${commentprefix}<author>",
            "${commentprefix}  ${name}",
            "${commentprefix}  ${email}",
            "${commentprefix}</author>",
            "${commentend}"
        ]
    },
    "window.zoomLevel": 1,
    "editor.unicodeHighlight.nonBasicASCII": false,
    "editor.unicodeHighlight.invisibleCharacters": false,
    "editor.unicodeHighlight.ambiguousCharacters": false,
    "omnisharp.enableDecompilationSupport": true,
    "editor.insertSpaces": false,
    "editor.detectIndentation": true,
    "editor.tokenColorCustomizations": {
        "[Default Dark+]": {
            "textMateRules": [
                {
                    "scope": "comment.documentation",
                    "settings": {
                        "foreground": "#608B4E",
                    }
                },
                {
                    "scope": "comment.documentation.attribute",
                    "settings": {
                        "foreground": "#C8C8C8",
                    }
                },
                {
                    "scope": "comment.documentation.cdata",
                    "settings": {
                        "foreground": "#E9D585",
                    }
                },
                {
                    "scope": "comment.documentation.name",
                    "settings": {
                        "foreground": "#569CD6",
                    }
                },
            ]
        }
    },
    "diffEditor.renderSideBySide": false,
    "diffEditor.hideUnchangedRegions.enabled": true
}
